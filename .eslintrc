{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "restParams": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": 0,    
    "no-unused-vars" : 2,
    "no-undef" : 2
  },
  "parserOptions": {
      "sourceType": "module"
  }
}